# Depends on: xfce4-dev-tools libxfce4util xfconf libxfce4ui garcon exo xfce4-panel thunar xfce4-settings xfce4-session xfwm4 xfdesktop xfce4-appfinder gtk-xfce-engine-2 tumbler

name=xfce
version=1.13
release=1
source=()

build() {
  install -d -m 0755 $PKG/etc
  echo "Xfce $version" > $PKG/etc/xfce_version
  
}
