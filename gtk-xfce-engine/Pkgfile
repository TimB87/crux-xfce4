# Description:	Gtk engine
# URL:		https://www.xfce.org
# Maintainer:	jolupa, jolupameister at gmail dot com
# Depends on:	gtk3

name=gtk-xfce-engine
version=3.2.0
release=2
source=(https://git.xfce.org/archive/$name/snapshot/$name-$version.tar.gz)

build() {

	cd $name-$version
	
	./autogen.sh \
	--prefix=/usr \
	--libexecdir=/usr/lib/$name \
	--disable-static \
	
	make

	make DESTDIR=$PKG install
}
