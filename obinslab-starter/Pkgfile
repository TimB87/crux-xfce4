# Description: Control your AnnePro Keyboad
# URL: http://en.obins.net/obinslab-starter/
# Maintainer: jolupa, jolupameister at gmail dot com
# Depends on: gtk xorg-libxscrnsaver xorg-libxtst libxkbcommon nss libappindicator-gtk2

name=obinslab-starter
_name=ObinsKit
_name_install=obinskit
version=1.0.11
release=2
source=(http://releases.obins.net/occ/linux/deb/ObinslabStarter_"$version"_x64.deb)

build() {
  
  ar xf ObinslabStarter_"$version"_x64.deb
  tar xf data.tar.xz -C "$PKG"

  cp $SRC/install.sh $PKG/opt/"Obinslab Starter"/
  
  rm -r $PKG/usr/share/doc
  rm  $PKG/opt/Obinslab\ Starter/{LICENSE.electron.txt,LICENSES.chromium.html}
  
}
